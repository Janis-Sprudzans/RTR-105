#include <stdio.h>

int fact(int x)
{
        if (x<0)
        {
                return -1;
        }
        else if (x==0)
        {
                return 1;
        }
        else if (x==1)
        {
                return 1;
        }
        else
        {
                for (int i=2;i<x;i++)
                {
                        x*=i;
                }
                return x;
        }


}

int main()
{

        printf("%d",fact(4));
        return 0;
}
